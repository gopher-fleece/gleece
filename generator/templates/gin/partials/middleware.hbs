{{#each Middlewares}}
	{{#equal Execution MiddlewaresExecution }}
		{{#if isErrorMiddleware}}
		if continueOperation := MiddlewarePkg{{@index}}.{{{FunctionName}}}(ctx, opError); continueOperation == false {
			return
		}
		{{else}}
		if continueOperation := MiddlewarePkg{{@index}}.{{{FunctionName}}}(ctx); continueOperation == false {
			return
		}
		{{/if}}
	{{/equal}}
{{/each}}
