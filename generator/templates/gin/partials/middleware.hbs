// Middlewares {{middlewares}} section
for _, middleware := range {{middlewares}} {
	{{#if isErrorMiddleware}}
		middlewareCtx, continueOperation := middleware(getRequestContext(ginCtx), ginCtx, {{errorName}})
		setRequestContext(ginCtx, middlewareCtx)
		if !continueOperation {
			return
		}
	{{else}}
		middlewareCtx, continueOperation := middleware(getRequestContext(ginCtx), ginCtx)
		setRequestContext(ginCtx, middlewareCtx)
		if !continueOperation {
			return
		}
	{{/if}}
}
// End middlewares {{middlewares}} section
