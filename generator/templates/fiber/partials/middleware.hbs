// Middlewares {{middlewares}} section
for _, middleware := range {{middlewares}} {
	{{#if isErrorMiddleware}}
		middlewareCtx, continueOperation := middleware(getRequestContext(fiberCtx), fiberCtx, {{errorName}})
		setRequestContext(fiberCtx, middlewareCtx)
		if !continueOperation {
			return nil
		}
	{{else}}
		middlewareCtx, continueOperation := middleware(getRequestContext(fiberCtx), fiberCtx)
		setRequestContext(fiberCtx, middlewareCtx)
		if !continueOperation {
			return nil
		}
	{{/if}}
}
// End middlewares {{middlewares}} section
